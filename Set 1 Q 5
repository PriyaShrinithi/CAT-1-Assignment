#include <iostream>
#define max 124;
using namespace std;
struct node {
	int value;
	struct node* next;
};

class sll
{
	struct node* top;
public:
	sll();
	void push(int x);
	int count();
	int count(int j);
	int sort();
};
sll::sll()
{
	top == NULL;
	top->next == NULL;
}
void sll::push(int x)
{
	struct node* temp;
	temp = new node;
	temp->value = x;

	top = temp;
	temp->next = top->next;
	delete temp;
}

int sll::count()
{
	int c = 0;
	struct node* prev;
	struct node* curr;
	for (curr = top; curr->next != NULL; curr = curr->next)
	{
		for (prev = top; prev != NULL; prev = prev->next)
		{
			if (curr->value == prev->value)
			{
				c++;
				break;
			}
				
		}
	}

	return c;
}
int main()
{
	sll s;
	int j;
	for (int i = 0; i < 5; i++)
	{
		int x;
		cout << "Enter element " << i + 1 << " ";
		cin >> x;
		s.push(x);
	}
	
	int x = s.count();
	return 0;
}
